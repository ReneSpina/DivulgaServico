@model DIVULGA_SERVICOS.Models.CAD_CIDADES_DIVULGA_FORNECEDOR

@{
    ViewBag.Title = "Detalhes da Cidade";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<head>
    <script async src="//pagead2.googlesyndication.com/pagead/js/adsbygoogle.js"></script>
    <script>
        (adsbygoogle = window.adsbygoogle || []).push({
          google_ad_client: "ca-pub-2571448761678693",
          enable_page_level_ads: true
        });
    </script>
</head>
<h2>Detalhes da Cidade</h2>

<div>
    <dl class="dl-horizontal">
        <dt>
            Cidade
        </dt>

        <dd>
            @Html.DisplayFor(model => model.NM_CIDADE)
        </dd>

        <dt>
            Estado
        </dt>

        <dd>
            @Html.DisplayFor(model => model.NM_ESTADO)
        </dd>

        <dt>
            Nível Brasil
        </dt>

        <dd>
            @Html.DisplayFor(model => model.BRASIL)
        </dd>

    </dl>
</div>
<p>
    @Html.ActionLink("Voltar a lista de cidades", "CidadesFornecedor")
</p>
